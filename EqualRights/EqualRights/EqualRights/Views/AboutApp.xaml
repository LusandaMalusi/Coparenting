<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="http://prismlibrary.com"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="EqualRights.Views.AboutApp"
             BackgroundImageSource="Pageback.jpg">
    <StackLayout>
        <Label Text="Coparenting Services" 
           HorizontalOptions="CenterAndExpand"
           TextColor="Black"
           FontSize="25"
           Margin="25,20"
           FontAttributes="Bold"/>
        <Button Text="Get to know your Rights" Command="{Binding OpenPdfCommand}"/>
        
    </StackLayout>
  
</ContentPage>